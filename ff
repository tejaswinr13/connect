import okhttp3.mockwebserver.MockResponse;
import okhttp3.mockwebserver.MockWebServer;
import org.junit.jupiter.api.Test;
import org.springframework.web.reactive.function.client.WebClient;
import reactor.core.publisher.Mono;

import java.io.IOException;

import static org.junit.jupiter.api.Assertions.assertNotNull;

public class YourTestClass {

    @Test
    void getEnrollmentDoc() throws IOException {
        MockWebServer mockServer = new MockWebServer();
        mockServer.start();

        String httpUrl = mockServer.url("/").toString();
        String onlineCustomerId = "b2c5f9eb-bbf7-4281-97ee-c2534299d08e";

        WebClient webClient = WebClient.create(httpUrl);
        HPCoreServiceImpl hpCoreService = new HPCoreServiceImpl(ccmConfig, webClient);

        mockServer.enqueue(
                new MockResponse()
                        .setResponseCode(200)
                        .addHeader("Content-Type", "application/octet-stream")
                        .setBody("your_response_body_here")
        );

        Mono<byte[]> enrollmentDocMono = hpCoreService.getEnrollmentDoc(onlineCustomerId);
        byte[] enrollmentDocBytes = enrollmentDocMono.block();

        assertNotNull(enrollmentDocBytes);

        mockServer.shutdown();
    }

    // Your HPCoreServiceImpl class implementation
}
